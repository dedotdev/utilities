{
  "name": "@dedot/chain-specs",
  "version": "0.0.1",
  "description": "Substrate-connect well known chain specifications",
  "author": "Tung Vu <tung@dedot.dev>",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "build-js-specs": "node scripts/build-js-specs.js",
    "build": "yarn clean && yarn build-js-specs && tsc --project tsconfig.build.json && tsc --project tsconfig.build.cjs.json && rm -rf ./dist/tsconfig.build.cjs.tsbuildinfo",
    "clean": "rm -rf ./dist && rm -rf ./tsconfig.tsbuildinfo ./tsconfig.build.tsbuildinfo ./tsconfig.build.cjs.tsbuildinfo"
  },
  "devDependencies": {
    "@dedot/providers": "^0.18.5",
    "smoldot": "^2.0.39",
    "typescript": "5.6.2"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "license": "MIT"
}
